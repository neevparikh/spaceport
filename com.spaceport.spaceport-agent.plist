<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.spaceport.spaceport-agent</string>
  <key>ProgramArguments</key>
  <array>
    <string>open</string>
    <string>/Applications/spaceport.app</string>
  </array>
	<key>RunAtLoad</key>
	<true/>
  <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
 	     <false/>
 	     <key>Crashed</key>
 	     <true/>
    </dict>
  <key>StandardOutPath</key>
  <string>/tmp/spaceport_neev.out.log</string>
  <key>StandardErrorPath</key>
  <string>/tmp/spaceport_neev.err.log</string>
  <key>ProcessType></key>
  <string>Background</string>
</dict>
</plist>
